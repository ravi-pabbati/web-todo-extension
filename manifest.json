{
  "name": "Web TODO",
  "description": "A simple way to organize your web bookmarks by priority",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "web-todo-icon.png",
      "96": "web-todo-icon.png",
      "128": "web-todo-icon-128.png"
    }
  },
  "icons": {
    "48": "web-todo-icon.png",
    "96": "web-todo-icon.png",
    "128": "web-todo-icon-128.png"
  }
}
